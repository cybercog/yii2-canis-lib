{
	"name": "infiniteCascade/yii2-infinite-core",
	"description": "Core libraries used in Infinite Cascade Yii2 based products",
	"keywords": ["yii", "libraries"],
	"homepage": "http://www.infinitecascade.com",
	"type": "yii2-extension",
	"license": "GPL -2.0",
	"minimum-stability": "dev",
	"require": {
		"php": ">=5.4.0",
		"yiisoft/yii2": "2.0.*",
		"yiisoft/yii2-redis": "2.0.*",
		"bower-asset/underscore": "1.7.*",
		"fortawesome/font-awesome": "*",
		"malsup/form": "3.*",
		"umpirsky/country-list": "*",
		"jom/php-subnational-division": "dev-master",
		"bootstrap-select/bootstrap-select": "*",
		"eternicode/bootstrap-datepicker": "*",
		"phpdocumentor/reflection-docblock": "2.0.*@dev",
        "bshaffer/oauth2-server-php": "v1.3",
        "bower-asset/smart-time-ago": "*"
	},
	"require-dev": {
		"phpunit/phpunit": "3.7.*",
		"fabpot/php-cs-fixer": "@stable"
	},
	"autoload": {
		"classmap": ["library/infinite/Infinite.php", "library/infinite/ICD.php"],
		"psr-0": { "infinite\\": "/library" }
	},
    "extra": {
            "bootstrap": "Infinite"
    }
}
