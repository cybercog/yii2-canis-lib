{
	"name": "infiniteCascade/yii2-infinite-core",
	"description": "Core libraries used in Infinite Cascade Yii2 based products",
	"keywords": ["yii", "libraries"],
	"homepage": "http://www.infinitecascade.com",
	"type": "yii2-extension",
	"license": "GPL -2.0",
	"minimum-stability": "dev",
	"require": {
		"php": ">=5.4.0",
		"yiisoft/yii2": "dev-master",
		"yiisoft/yii2-composer": "dev-master",
		"components/jquery": "1.10.*",
		"components/jqueryui": "1.10.*",
		"components/underscore": "1.5.*"
	},
	"config": {
        "component-dir": "library/infinite/assets/js"
    },
	"require-dev": {
		"phpunit/phpunit": "3.7.*"
	},
	"autoload": {
		"psr-0": { "infinite\\": "/library" }
	},
    "extra": {
            "bootstrap": "infinite\\Infinite"
    }
}
